{
  "env": { "jest": true, "browser": true },
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "airbnb", "prettier", "prettier/react"],
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }]
  },
  "settings": { "import/resolver": { "node": { "paths": ["./src"] } } }
}
