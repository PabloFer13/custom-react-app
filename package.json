{
  "name": "ferras",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=8",
    "yarn": ">=1"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint --fix .",
    "test": "react-scripts test --env=jsdom",
    "test:all": "yarn lint && yarn test --coverage",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
